<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    android:visibility="visible"
    tools:context=".FormCadastro">

    <View
        android:id="@+id/view_principal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/view_principal_cadastro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="110dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"

        />

    <View
        android:id="@+id/logo_psdesk"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:background="@drawable/logo_tela_cadastro"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/view_principal"
        android:layout_marginTop="110dp"
        />

    <TextView
        android:id="@+id/cadastro_texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastro"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/logo_psdesk"
        app:layout_constraintEnd_toEndOf="@id/logo_psdesk"
        app:layout_constraintTop_toBottomOf="@id/logo_psdesk"
        android:layout_marginTop="20dp"
        />

    <View
        android:id="@+id/first_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="40dp"
        android:layout_marginRight="40dp"
        android:background="@color/gray"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/cadastro_texto" />

    <View
        android:id="@+id/second_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/first_view"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"

        />

    <View
        android:id="@+id/third_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/second_view"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"

        />

    <View
        android:id="@+id/fourth_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/third_view"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"

        />

    <View
        android:id="@+id/fifth_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/fourth_view"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"

        />

    <View
        android:id="@+id/sixth_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/fifth_view"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"

        />

    <View
        android:id="@+id/seventh_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/gray"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/sixth_view"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"

        />


    <ImageView
        android:id="@+id/first_icon"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:src="@drawable/ic_baseline_account_circle_24"
        app:layout_constraintStart_toStartOf="@id/first_view"
        app:layout_constraintTop_toTopOf="@id/first_view" />

    <TextView
        android:id="@+id/nome_completo_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NOME COMPLETO"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/first_view"
        app:layout_constraintTop_toTopOf="@id/first_view"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="2dp"
        />

    <EditText
        android:id="@+id/nome_completo"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintStart_toStartOf="@id/nome_completo_text"
        app:layout_constraintTop_toBottomOf="@id/nome_completo_text"
        app:layout_constraintBottom_toBottomOf="@id/first_view"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="60dp"
        android:inputType="textPersonName"
        />

    <ImageView
        android:id="@+id/second_icon"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:src="@drawable/ic_baseline_call_24"
        app:layout_constraintStart_toStartOf="@id/second_view"
        app:layout_constraintTop_toTopOf="@id/second_view" />

    <TextView
        android:id="@+id/telefone_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TELEFONE"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/second_view"
        app:layout_constraintTop_toTopOf="@id/second_view"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="2dp"
        />

    <EditText
        android:id="@+id/telefone"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintStart_toStartOf="@id/telefone_text"
        app:layout_constraintTop_toBottomOf="@id/telefone_text"
        app:layout_constraintBottom_toBottomOf="@id/second_view"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="60dp"
        android:inputType="phone"
        />

    <ImageView
        android:id="@+id/third_icon"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:src="@drawable/ic_calendar"
        app:layout_constraintStart_toStartOf="@id/third_view"
        app:layout_constraintTop_toTopOf="@id/third_view" />

    <TextView
        android:id="@+id/data_nascimento_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DATA DE NASCIMENTO"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/third_view"
        app:layout_constraintTop_toTopOf="@id/third_view"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="2dp"
        />

    <EditText
        android:id="@+id/data_nascimento"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintStart_toStartOf="@id/data_nascimento_text"
        app:layout_constraintTop_toBottomOf="@id/data_nascimento_text"
        app:layout_constraintBottom_toBottomOf="@id/third_view"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="60dp"
        android:inputType="date"
        />


    <ImageView
        android:id="@+id/fourth_icon"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:src="@drawable/ic_email"
        app:layout_constraintStart_toStartOf="@id/fourth_view"
        app:layout_constraintTop_toTopOf="@id/fourth_view" />

    <TextView
        android:id="@+id/email_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-MAIL"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/fourth_view"
        app:layout_constraintTop_toTopOf="@id/fourth_view"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="2dp"
        />

    <EditText
        android:id="@+id/email"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintStart_toStartOf="@id/email_text"
        app:layout_constraintTop_toBottomOf="@id/email_text"
        app:layout_constraintBottom_toBottomOf="@id/fourth_view"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="60dp"
        android:inputType="textEmailAddress"
        />

    <ImageView
        android:id="@+id/fifth_icon"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:src="@drawable/ic_key"
        app:layout_constraintStart_toStartOf="@id/fifth_view"
        app:layout_constraintTop_toTopOf="@id/fifth_view" />

    <TextView
        android:id="@+id/chave_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CHAVE DE ACESSO"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/fifth_view"
        app:layout_constraintTop_toTopOf="@id/fifth_view"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="2dp"
        />

    <EditText
        android:id="@+id/chave_acesso"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintStart_toStartOf="@id/chave_text"
        app:layout_constraintTop_toBottomOf="@id/chave_text"
        app:layout_constraintBottom_toBottomOf="@id/fifth_view"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="60dp"
        android:inputType="textCapCharacters"
        />

    <ImageView
        android:id="@+id/sixth_icon"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:src="@drawable/ic_baseline_lock_24"
        app:layout_constraintStart_toStartOf="@id/sixth_view"
        app:layout_constraintTop_toTopOf="@id/sixth_view" />

    <TextView
        android:id="@+id/senha_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SENHA"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/sixth_view"
        app:layout_constraintTop_toTopOf="@id/sixth_view"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="2dp"
        />

    <EditText
        android:id="@+id/senha"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintStart_toStartOf="@id/senha_text"
        app:layout_constraintTop_toBottomOf="@id/senha_text"
        app:layout_constraintBottom_toBottomOf="@id/sixth_view"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="60dp"
        android:drawableRight="@drawable/ic_eye"
        android:inputType="textPassword"
        />

    <ImageView
        android:id="@+id/seventh_icon"
        android:layout_width="45dp"
        android:layout_height="50dp"
        android:layout_marginLeft="15dp"
        android:src="@drawable/ic_baseline_lock_24"
        app:layout_constraintStart_toStartOf="@id/seventh_view"
        app:layout_constraintTop_toTopOf="@id/seventh_view" />

    <TextView
        android:id="@+id/confirmar_senha_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONFIRMAR SENHA"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/seventh_view"
        app:layout_constraintTop_toTopOf="@id/seventh_view"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="2dp"
        />

    <EditText
        android:id="@+id/confirmar_senha"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        app:layout_constraintStart_toStartOf="@id/confirmar_senha_text"
        app:layout_constraintTop_toBottomOf="@id/confirmar_senha_text"
        app:layout_constraintBottom_toBottomOf="@id/seventh_view"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="60dp"
        android:drawableRight="@drawable/ic_eye"
        android:inputType="textPassword"
        />


    <CheckBox
        android:id="@+id/aceitoTermos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Li e concordo com os"
        app:layout_constraintStart_toStartOf="@id/view_principal"
        app:layout_constraintEnd_toEndOf="@id/view_principal"
        app:layout_constraintTop_toBottomOf="@id/seventh_view"
        android:layout_marginTop="30dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_cadastro"
        style="@style/Button_cadastro"
        android:text="Cadastrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/aceitoTermos"
        />






</androidx.constraintlayout.widget.ConstraintLayout>