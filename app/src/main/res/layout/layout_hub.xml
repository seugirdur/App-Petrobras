<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Hub"
    android:theme="@style/Theme.AppCompat.Light"
    android:background="@color/white">

    <TextView
        android:id="@+id/tex0"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:onClick="rediriciona0"
        android:text="Tela de Escolha"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tex1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="Login"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex0"
        android:onClick="rediriciona1"/>

    <TextView
        android:id="@+id/tex2"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="Cadastro"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex1"
        android:onClick="rediriciona2"/>

    <TextView
        android:id="@+id/tex3"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="TabActivity"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex2"
        android:onClick="rediriciona3"/>

    <TextView
        android:id="@+id/tex4"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="ProblemActivity"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex3"
        android:onClick="rediriciona4"/>

    <TextView
        android:id="@+id/tex5"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="RelatorioProcesso"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex4"
        android:onClick="rediriciona5"/>

    <TextView
        android:id="@+id/tex6"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="Ajuda"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex5"
        android:onClick="rediriciona6"/>

    <TextView
        android:id="@+id/tex7"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="Administrador"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex6"
        android:onClick="rediriciona7"/>

    <TextView
        android:id="@+id/tex8"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="Splash Screen"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex7"
        android:onClick="rediriciona8"/>

    <TextView
        android:id="@+id/tex9"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="SoluctionActivity"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex8"
        android:onClick="rediriciona9"/>

    <TextView
        android:id="@+id/tex10"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="perfil"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex9"
        android:onClick="rediriciona10"/>

    <TextView
        android:id="@+id/tex11"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="Tela Pedro"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/tex10"
        android:onClick="rediriciona11"/>
</androidx.constraintlayout.widget.ConstraintLayout>