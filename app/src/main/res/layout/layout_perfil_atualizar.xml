<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view2"
        android:layout_width="358dp"
        android:layout_height="590dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/view_tela_login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="15dp"
        android:foregroundGravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:srcCompat="@drawable/ic_user_perfil"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="47dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="75dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/editText3"
        app:layout_constraintBottom_toBottomOf="@id/emailupdate"
        app:srcCompat="@drawable/ic_baseline_email_24"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="75dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/editText1"
        app:layout_constraintBottom_toBottomOf="@id/nomecompletoupdate"
        app:srcCompat="@drawable/ic_baseline_account_circle_24"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="75dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/editText2"
        app:layout_constraintBottom_toBottomOf="@id/num_telupdate"
        app:srcCompat="@drawable/ic_baseline_call_preto"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/num_chave"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginStart="92dp"
        android:layout_marginEnd="26dp"
        android:hint="Chave"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/editText4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editText4"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="26dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Registro do Funcionário"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/emailupdate"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/emailupdate"
        android:layout_width="240dp"
        android:layout_height="45dp"
        android:layout_marginStart="88dp"
        android:layout_marginEnd="25dp"
        android:hint="E-mail"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editText3"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="25dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="E-mail"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/num_telupdate"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/num_telupdate"
        android:layout_width="240dp"
        android:layout_height="45dp"
        android:layout_marginStart="88dp"
        android:layout_marginEnd="26dp"
        android:hint="Número de Telefone"
        android:maxLength="15"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/editText2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editText2"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="26dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Telefone"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/nomecompletoupdate"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/nomecompletoupdate"
        android:layout_width="240dp"
        android:layout_height="45dp"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="25dp"
        android:hint="Nome completo"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/editText1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editText1"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginStart="93dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="25dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nome Completo"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/nome_login"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_tela_perfil_update"
        style="@style/btn_tela_perfil"
        android:layout_marginTop="30dp"
        android:drawableStart="@drawable/ic_conf"
        android:text="CONCLUIR ALTERAÇÃO"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintTop_toBottomOf="@+id/num_chave"
        tools:ignore="MissingConstraints"
        />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="@string/ppja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_tela_perfil_update"
        tools:ignore="MissingConstraints,UnknownId" />

    <!--style="@style/btn_tela_login"-->

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="75dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@id/editText4"
        app:layout_constraintBottom_toBottomOf="@id/num_chave"
        app:srcCompat="@drawable/ic_resgis_do_func"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nome_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nome_do_login"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>